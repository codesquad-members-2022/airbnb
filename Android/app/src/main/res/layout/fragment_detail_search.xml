<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.search.detail.DetailSearchFragment">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/detail_nav_host"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:defaultNavHost="true"
            app:navGraph="@navigation/detail_search_graph"
            app:layout_constraintBottom_toTopOf="@id/btn_detail_search"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/btn_detail_search"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:enabled="true"
            app:layout_constraintHeight_percent="0.07"
            app:layout_constraintWidth_percent="0.3"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="25sp"
            android:gravity="center"
            android:text="건너뛰기"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>