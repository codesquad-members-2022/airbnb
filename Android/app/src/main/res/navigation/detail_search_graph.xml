<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/detail_search_graph"
    app:startDestination="@id/calendarFragment">

    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.team16.airbnb.ui.calendar.CalendarFragment"
        android:label="CalendarFragment" >
        <action
            android:id="@+id/action_calendarFragment_to_moneyRangeFragment"
            app:destination="@id/moneyRangeFragment" />
    </fragment>
    <fragment
        android:id="@+id/moneyRangeFragment"
        android:name="com.team16.airbnb.MoneyRangeFragment"
        android:label="fragment_money_range"
        tools:layout="@layout/fragment_money_range" >
        <action
            android:id="@+id/action_moneyRangeFragment_to_personFragment"
            app:destination="@id/personFragment" />
        <action
            android:id="@+id/action_moneyRangeFragment_to_calendarFragment"
            app:destination="@id/calendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/personFragment"
        android:name="com.team16.airbnb.PersonFragment"
        android:label="fragment_person"
        tools:layout="@layout/fragment_person" >
        <action
            android:id="@+id/action_personFragment_to_moneyRangeFragment"
            app:destination="@id/moneyRangeFragment" />
    </fragment>
</navigation>